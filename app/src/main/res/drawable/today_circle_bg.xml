<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="@android:color/transparent" />
            <solid android:color="@color/caldroid_transparent" />
            <padding android:bottom="1dp" />
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <stroke
                android:width="@dimen/dpw2"
                android:color="@android:color/transparent"/>
            <solid android:color="@color/c5d5d5d" />
            <size
                android:width="@dimen/dpw12"
                android:height="@dimen/dpw12"/>
        </shape>
    </item>


</layer-list>